{
  "name": "trojans-ec-bn",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon -L --exec babel-node ./src/index.js",
    "migrate": "npx sequelize-cli db:migrate",
    "down": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "undo-seed": "npx sequelize-cli db:seed:undo",
    "test": "cross-env NODE_ENV=test  npm run migrate:all && cross-env NODE_ENV=test jest  --forceExit --collect-coverage && coveralls < coverage/lcov.info",
    "migrate:all": "npm run down && npm run migrate && npm run seed",
    "lint": "npx eslint .",
    "lint-fix": "npx eslint --fix ./",
    "prepare": "husky install",
    "docker-start-up": "docker-compose -f docker-compose.yml up -d && docker exec trojans-ec-bn-node-app-1 npm run migrate:all",
    "docker-up": "docker-compose -f docker-compose.yml up -d",
    "docker-down": "docker-compose -f docker-compose.yml down",
    "docker-volume-prune": "docker volume prune"
  },
  "jest": {
    "resetMocks": true,
    "restoreMocks": true,
    "resetModules": true,
    "testTimeout": 120000
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlp-rwanda/trojans-ec-bn.git"
  },
